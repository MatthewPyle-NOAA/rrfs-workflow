 icoord             =sigma;       # vertical coordinate (1=isentropic,2=sigma,3=p,4=z)
 idomain            =1;           # identifies subdomains
 map_proj           =PROJ_ROTLL;  # identifies map projection
 dxm                =1924.871;    # model dx (m)
 dym                =1924.871;    # model dy (m)
 truelat1           =35.0;        # projection truelat1 -> pollat for PROJ_ROTLL
 truelat2           =67.0;        # projection truelat2 -> pollon for PROJ_ROTLL
 stdlon             =0.0;         # projection stand lon -> gridrot for PROJ_ROTLL
